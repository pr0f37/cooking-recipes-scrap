        jQuery(function ($) {
            $.validator.addMethod('date',
            function (value, element) {
                if (this.optional(element)) {
                    return true;
                }

                var ok = true;
                try {

                    var myRegExp = new RegExp(/\d{2}-\d{2}-\d{4}$/);

                    return value.match(myRegExp);


                }
                catch (err) {
                    alert(err);
                    ok = false;
                }
                return ok;
            });
        });


