          if($(this).hasClass('przepisy')){ h='560px';}
          if($(this).hasClass('tradycyjnie')){ h='245px';}
          if($(this).hasClass('abc')){ h='245px';}

          $(this).find('a:first').next('.submenu_panel').addClass('selected').stop().animate({"height": h, "padding-top": "30px", "padding-bottom": "30px" }, 400);
          /*var i = 400;
          $(this).find('a:first').next('.submenu_panel').find('li').each(function () {
